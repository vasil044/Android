<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/contenedorInformacionCreamasLand7"
    tools:context="co.quindio.sena.senasoftquindio2016.fragments.ContenedorInfoCreamasFragment">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/logo_completo"
        android:layout_marginLeft="10dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:text=" "
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/colorPrimary"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <fragment
            android:id="@+id/idFragmentQueEsCreamas"
            android:name="co.quindio.sena.senasoftquindio2016.fragments.CreamasQueesFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:layout_marginBottom="20dp"
            tools:layout="@layout/fragment_creamas_quees"/>

        <fragment
            android:id="@+id/idFragmentProjects"
            android:name="co.quindio.sena.senasoftquindio2016.fragments.ProyectosCreamasFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            tools:layout="@layout/fragment_proyectos_creamas"/>

    </LinearLayout>

</LinearLayout>
